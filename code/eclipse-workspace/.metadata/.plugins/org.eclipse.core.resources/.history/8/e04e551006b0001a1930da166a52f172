package com.inesshasnaoui.bibliotheekbeheersysteem.dao.InterfaceDao;

import java.util.List;

/**
 * Base generic context containing all crud options
 * @param <T>
 */

public interface CrudDao<T> {
	
	    T insert(T obj);
	    T update(T obj);
	    boolean delete(T obj);
	    T get(long id);
	    List<T> getAll();
	
	    
	  
}
